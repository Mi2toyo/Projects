--// New method, Lerk or Nighter dont patch this pls im just tryna get some fame
--[[
getgenv().Settings = {
    ['Mode'] = '', --[ Faded / Swagmode ]
    ['Anti Execution Logger'] = false,
    ['Anti Premium'] = false
}
]]

if Settings.Mode:lower() == 'faded' then
    if Settings['Anti Execution Logger'] then
        local antiExecLogger; antiExecLogger = hookmetamethod(game, '__namecall', newcclosure(function(...)
            if getnamecallmethod():lower():find'httpget' and ({...})[2]:find'.php?' then
                return
            end
            return antiExecLogger(...)
        end))
    end
    task.wait(.1)
    do
        loadstring(game:HttpGet('https://raw.githubusercontent.com/NighterEpic/Faded/main/YesEpic', true))()
    end
    if Settings['Anti Premium'] then
        for i, c in pairs(getconnections(game:FindService'Players'.localPlayer.PlayerGui.BubbleChat.ChildAdded)) do
            if i ~= 2 then
                c:Disable()
            end
        end
    end
elseif Settings.Mode:lower() == 'swagmode' then
    if Settings['Anti Execution Logger'] then
        local antiExecLogger; antiExecLogger = hookmetamethod(game, '__namecall', newcclosure(function(...)
            if getnamecallmethod():lower():find'httpget' and ({...})[2]:find'.php?' then
                return
            end
            return antiExecLogger(...)
        end))
    end
    task.wait(.1)
    do
        loadstring(game:HttpGet'https://raw.githubusercontent.com/lerkermer/lua-projects/master/SwagModeV002')()
    end
    if Settings['Anti Premium'] then
        coroutine.wrap(function()
            local owners, premUsers = loadstring(game:HttpGet'https://raw.githubusercontent.com/swagmode/swagmode/main/swagdif')(), loadstring('return{'..game:HttpGet'https://raw.githubusercontent.com/swagmode/swagmode/main/swagmain':split'ModIDS = {'[2]:split'}'[1]..'}')()
            for _, p in pairs(game:FindService'Players':GetPlayers()) do
                if table.find(owners, p.UserId) or table.find(premUsers, p.UserId) then
                    for i, c in pairs(getconnections(p.Chatted)) do
                        if i ~= 1 then
                            c:Disable()
                        end
                    end
                end
            end
            game:FindService'Players'.PlayerAdded:connect(function(p)
                if table.find(owners, p.Name) or table.find(premUsers, p.UserId) then
                    for i, c in pairs(getconnections(p.Chatted)) do
                        if i ~= 1 then
                            c:Disable()
                        end
                    end
                end
            end)
        end)()
    end
else
    game:FindService'Players'.localPlayer:kick'Invalid mode.'
end
